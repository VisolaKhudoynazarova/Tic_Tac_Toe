<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="idrok.visola_khudoynazarova.tictactoe.Fragments.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_mainFragment2"
            app:destination="@id/mainFragment" />
    </fragment>

    <fragment
        android:id="@+id/mainFragment"
        android:name="idrok.visola_khudoynazarova.tictactoe.Fragments.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_gameFragment"
            app:destination="@id/gameFragment" />
    </fragment>

    <fragment
        android:id="@+id/gameFragment"
        android:name="idrok.visola_khudoynazarova.tictactoe.Fragments.GameFragment"
        android:label="fragment_game"
        tools:layout="@layout/fragment_game" >
<!--         <action-->
<!--            android:id="@+id/action_gameFragment_to_myDialog"-->
<!--            app:destination="@id/myDialog" -->
<!--             />//keremas bu-->
    </fragment>

    <fragment
        android:id="@+id/gameFragment2"
        android:name="idrok.visola_khudoynazarova.tictactoe.Fragments.GameFragment2"
        android:label="fragment_game2"
        tools:layout="@layout/fragment_game2" />

    <fragment
        android:id="@+id/infoFragment"
        android:name="idrok.visola_khudoynazarova.tictactoe.Fragments.InfoFragment"
        android:label="fragment_info"
        tools:layout="@layout/fragment_info" />

    <fragment
        android:id="@+id/settingFragment"
        android:name="idrok.visola_khudoynazarova.tictactoe.Fragments.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />

</navigation>